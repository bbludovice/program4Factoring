#include <stdio.h>


int get_value (void)
{
	 int value;
 
 do {
 	 printf ("Please type in a positive integer:  ");
 	 scanf ("%i", &value);
 	 printf ("You typed in %i.\n", value);
    } while (value < 0);
 printf ("%i is a good value.\n", value);
 return value;	
}


